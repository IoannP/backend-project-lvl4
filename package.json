{
   "name": "@hexlet/code",
   "version": "0.1.0",
   "description": "fastify-nodejs-application",
   "main": "server/index.js",
   "scripts": {
      "test": "jest",
      "prebuild": "rm -rf dist",
      "build-configs": "babel webpack.config.babel.js --out-dir dist",
      "build-server": "NODE_ENV=production babel server --out-dir dist/server --source-maps inline && cp -r server/views dist/server/views",
      "build-frontend": "NODE_ENV=production webpack --mode production",
      "build": "npm run build-configs && npm run build-frontend && npm run build-server",
      "start": "NODE_ENV=production node dist/server/bin/server.js"
   },
   "engines": {
      "node": "14.x"
   },
   "jest": {
      "modulePathIgnorePatterns": [
         "<rootDir>/__tests__/helpers/"
      ]
   },
   "author": "Ioann Pilipchuk",
   "license": "ISC",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/IoannP/backend-project-lvl4.git"
   },
   "bugs": {
      "url": "https://github.com/IoannP/backend-project-lvl4/issues"
   },
   "homepage": "https://github.com/IoannP/backend-project-lvl4#readme",
   "dependencies": {
      "bootstrap": "^4.6.0",
      "fastify": "^3.12.0",
      "fastify-static": "^4.0.1",
      "i18next": "^19.8.9",
      "jquery": "^3.5.1",
      "mini-css-extract-plugin": "^1.3.8",
      "pino-pretty": "^4.7.0",
      "point-of-view": "^4.14.0",
      "pug": "^3.0.2",
      "webpack": "^5.23.0"
   },
   "devDependencies": {
      "@babel/cli": "^7.12.17",
      "@babel/core": "^7.12.17",
      "@babel/node": "^7.12.17",
      "@babel/preset-env": "^7.12.17",
      "@jest/globals": "^26.6.2",
      "babel-eslint": "^10.1.0",
      "babel-jest": "^26.6.3",
      "babel-loader": "^8.2.2",
      "css-loader": "^5.0.2",
      "eslint": "^7.20.0",
      "eslint-config-airbnb-base": "^14.2.1",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-jest": "^24.1.5",
      "jest": "^26.6.3",
      "jest-cli": "^26.6.3",
      "nodemon": "^2.0.7",
      "style-loader": "^2.0.0",
      "webpack-cli": "^4.5.0",
      "webpack-dev-server": "^3.11.2"
   }
}
